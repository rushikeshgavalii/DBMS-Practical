Code: Explicit Cursor
declare 
pid int;
pname varchar(50);
pr float;
qty int;
cursor c1(p float) is select * from product where price>=p;
begin
open c1(&p);
loop 
fetch c1 into pid,pname,pr,qty;
exit when c1%notfound;
dbms_output.put_line(pid||pname||pr||qty);
end loop;
close c1;
end;
