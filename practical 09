CODE: DELETE CURSOR
declare 
pid int;
pname varchar(50);
pr float;
qty int;
cursor c1(p float) is select * from product where price>p for update;
begin
open c1(&p);
loop 
fetch c1 into pid,pname,pr,qty;
exit when c1% notfound;
delete from product where current of c1;
dbms_output.put_line(pid||pname||pr||qty);
end loop;
close c1;
end;
/
